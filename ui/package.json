        "prettier-plugin-svelte": "^2.7.0",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "svelte": "^3.44.0",
        "svelte-check": "^2.7.1",
        "svelte-preprocess": "^4.10.7",
